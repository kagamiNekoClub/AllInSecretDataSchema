{
    "$schema": "http://json-schema.org/draft-04/schema#",
	"title": "ActorData Of All In Secret",
	"description": "秘语探寻模板信息文档",
    "type": "object",
    "properties": {
		"name": {
			"$id": "/properties/name",
			"description": "The name of actor",
			"type": "string"
		},
		"description": {
			"$id": "/properties/description",
			"description": "The description of actor",
			"type": "string"
        },
        "type":{
            "$id": "/properties/type",
			"description": "The type of actor",
            "type": "string",
            "enum":["出身模板", "职业模板", "特性模板"]
        },
        "only":{
            "$id":"/properties/only",
            "type": "boolean",
            "default": false
        }
    }
}